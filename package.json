{
    "name": "trailblazers-trakcer-api",
    "version": "1.0.0",
    "description": "Taiko Trailblazers Tracker API",
    "main": "index.js",
    "repository": "https://github.com/bhaktibuana/trailblazers-tracker-api.git",
    "author": "Bhakti Mega Buana <bhaktibuana19@gmail.com>",
    "license": "MIT",
    "private": true,
    "scripts": {
        "build:prod": "rm -rf __tests__/coverage && jest --clearCache && cross-env NODE_ENV=production jest && webpack",
        "build:dev": "rm -rf __tests__/coverage && jest --clearCache && cross-env NODE_ENV=development jest && tsc --project tsconfig.test.json",
        "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register ./src/index.ts",
        "start": "cross-env NODE_ENV=production node -r tsconfig-paths/register ./dist/index.js",
        "test": "rm -rf __tests__/coverage && jest --clearCache && cross-env NODE_ENV=development jest",
        "format": "prettier --write \"src/**/*.ts\""
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/mongoose": "^5.11.97",
        "copy-webpack-plugin": "^12.0.2",
        "cross-env": "^7.0.3",
        "eslint": "^9.9.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "jest": "^29.7.0",
        "prettier": "^3.3.3",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.1",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.5.4",
        "webpack": "^5.94.0",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "dayjs": "^1.11.13",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "helmet": "^7.1.0",
        "http-status-codes": "^2.3.0",
        "mongoose": "^8.5.4"
    }
}
